FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      UIon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    erf1
    {
        type            fixedGradient;
        gradient        uniform (0 0 0);
    }
    erf2
    {
        type            fixedGradient;
        gradient        uniform (0 0 0);
    }
    es1
    {
        type            fixedGradient;
        gradient        uniform (0 0 0);
    }
    es2
    {
        type            fixedGradient;
        gradient        uniform (0 0 0);
    }
}


